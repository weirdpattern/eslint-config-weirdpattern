{
  "verbose": true,
  "testRegex": "(test[s]?|spec[s]?)/.*[\\.\\-][Ss]pec[s]?\\.js$",
  "coverageDirectory": "<rootDir>/coverage",
  "coveragePathIgnorePatterns": ["/node_modules/", "/specs/"],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  }
}
